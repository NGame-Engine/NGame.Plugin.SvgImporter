<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup>
        <_SvgImporter_TaskFolder>$(MSBuildThisFileDirectory)..\tasks\net8.0</_SvgImporter_TaskFolder>
        <SvgImporterAssembly>$(_SvgImporter_TaskFolder)\$(MSBuildThisFileName).dll</SvgImporterAssembly>
    </PropertyGroup>

    <UsingTask TaskName="$(MSBuildThisFileName).WriteSvgToPng" AssemblyFile="$(SvgImporterAssembly)"/>

</Project>
